{
  "name": "convulator",
  "version": "0.0.1",
  "description": "Converter and calculator",
  "main": "index.js",
  "scripts": {
    "pretest": "standard --verbose --fix",
    "test": "jest",
    "watch": "jest --watch",
    "production": "webpack --mode production",
    "start": "webpack-dev-server --content-base dist/ --progress --open --define process.env.NODE_ENV=\"'development'\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/blgm/convulator.git"
  },
  "keywords": [
    "calculator",
    "calculate",
    "coverter",
    "convert"
  ],
  "author": "George Blue",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/blgm/convulator/issues"
  },
  "homepage": "https://gitlab.com/blgm/convulator#README",
  "dependencies": {
    "@babel/core": "7.15.4",
    "@babel/preset-env": "7.15.4",
    "@babel/preset-react": "7.14.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.4.0",
    "babel-loader": "8.2.2",
    "big.js": "6.1.1",
    "copy-webpack-plugin": "9.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "jest": "27.1.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.4",
    "react-test-renderer": "16.14.0",
    "react-transition-group": "4.4.2",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-bind-selectors": "1.1.11",
    "redux-logger": "3.0.6",
    "regenerator-runtime": "0.13.9",
    "reselect": "4.0.0",
    "standard": "16.0.3",
    "typestyle": "2.1.0",
    "webpack": "5.52.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "<rootDir>/src/test-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "jsdom"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
