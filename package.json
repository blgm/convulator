{
  "name": "convulator",
  "version": "0.0.1",
  "description": "Converter and calculator",
  "main": "index.js",
  "scripts": {
    "pretest": "standard --verbose --fix",
    "test": "jest",
    "watch": "jest --watch",
    "production": "webpack -p",
    "start": "webpack-dev-server --content-base dist/ --progress --open --define process.env.NODE_ENV=\"'development'\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/blgm/convulator.git"
  },
  "keywords": [
    "calculator",
    "calculate",
    "coverter",
    "convert"
  ],
  "author": "George Blue",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/blgm/convulator/issues"
  },
  "homepage": "https://gitlab.com/blgm/convulator#README",
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "big.js": "5.2.2",
    "copy-webpack-plugin": "4.6.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "jest": "23.6.0",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "react-transition-group": "2.5.2",
    "react-test-renderer": "16.7.0",
    "redux": "4.0.1",
    "redux-actions": "2.6.4",
    "redux-bind-selectors": "1.1.3",
    "redux-logger": "3.0.6",
    "regenerator-runtime": "0.13.1",
    "reselect": "4.0.0",
    "standard": "12.0.1",
    "typestyle": "2.0.1",
    "webpack": "4.28.2",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "<rootDir>/src/test-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
