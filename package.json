{
  "name": "convulator",
  "version": "0.0.1",
  "description": "Converter and calculator",
  "main": "index.js",
  "scripts": {
    "pretest": "standard --verbose --fix",
    "test": "jest",
    "watch": "jest --watch",
    "production": "webpack -p",
    "start": "webpack-dev-server --content-base dist/ --progress --open --define process.env.NODE_ENV=\"'development'\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/blgm/convulator.git"
  },
  "keywords": [
    "calculator",
    "calculate",
    "coverter",
    "convert"
  ],
  "author": "George Blue",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/blgm/convulator/issues"
  },
  "homepage": "https://gitlab.com/blgm/convulator#README",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "big.js": "^4.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "enzyme-to-json": "3.1.2",
    "jest": "^21.0.2",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.0.0",
    "react-transition-group": "^2.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^2.2.1",
    "redux-bind-selectors": "^1.1.1",
    "redux-logger": "^3.0.6",
    "reselect": "^3.0.1",
    "standard": "^10.0.2",
    "typestyle": "^1.4.0",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "<rootDir>/src/test-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]

  }
}
