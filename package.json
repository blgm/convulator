{
  "name": "convulator",
  "version": "0.0.1",
  "description": "Converter and calculator",
  "main": "index.js",
  "scripts": {
    "pretest": "standard --verbose --fix",
    "test": "jest",
    "watch": "jest --watch",
    "production": "webpack -p",
    "start": "webpack-dev-server --content-base dist/ --progress --open --define process.env.NODE_ENV=\"'development'\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/blgm/convulator.git"
  },
  "keywords": [
    "calculator",
    "calculate",
    "coverter",
    "convert"
  ],
  "author": "George Blue",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/blgm/convulator/issues"
  },
  "homepage": "https://gitlab.com/blgm/convulator#README",
  "dependencies": {
    "babel-loader": "8.0.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "big.js": "5.1.2",
    "copy-webpack-plugin": "4.5.2",
    "enzyme": "3.5.0",
    "enzyme-adapter-react-16": "1.3.0",
    "enzyme-to-json": "3.3.4",
    "jest": "23.5.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-test-renderer": "16.4.2",
    "react-transition-group": "2.4.0",
    "redux": "4.0.0",
    "redux-actions": "2.6.1",
    "redux-bind-selectors": "1.1.2",
    "redux-logger": "3.0.6",
    "reselect": "3.0.1",
    "standard": "11.0.1",
    "typestyle": "2.0.1",
    "webpack": "4.17.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.6"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "<rootDir>/src/test-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
